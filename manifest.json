{
  "manifest_version": 3,
  "name": "Bolt - Learning Toolkit",
  "version": "1.0.0",
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "action": { "default_popup": "index.html" },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "https://*/*",
    "http://*/*"
  ],
  "host_permissions": ["https://*/*"]
}
